<h1>Listings</h1>
<table border="1" colspan="2">
 <thead>
   <tr><th>ID</th><th>Title</th><th>Description</th> </tr>
  </thead>
<tbody>
        <% @articles.each do |article| %>
        <tr> 
           <td><%= article.id %></td>
            <td>
            <%= article.title %>
            </td>
            <td>
            <%= article.description %></td>
        </tr>
        
        <% end %>
 </tbody>
</table>
